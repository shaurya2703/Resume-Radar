{
    "manifest_version": 3,
    "name": "Resume Radar",
    "version": "1.0",
    "description": "Match job descriptions with my resume.",
    "permissions": [
      "activeTab",
      "tabs",
      "storage"
    ],
    "action": {
      "default_popup": "popup.html",
      "default_icon": {
        "16": "images/icon1.png",
        "48": "images/icon1.png",
        "128": "images/icon1.png"
      }
    },
    "icons": {
      "16": "images/icon1.png",
      "48": "images/icon1.png",
      "128": "images/icon1.png"
    }
}